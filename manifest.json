{
  "manifest_version": 2,

  "name": "LinkSync",
  "description": "Save, archive, tag and group links using LinkSync.",
  "version": "0.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
